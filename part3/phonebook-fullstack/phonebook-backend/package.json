{
  "name": "phonebook-backend",
  "version": "1.0.0",
  "description": "Backend for phonebook",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "remove": "rm -rf ../phonebook-frontend/build && rm -rf ../phonebook-backend/build",
    "build:ui": "npm run remove && cd ../phonebook-frontend/ && npm run build && cp -r build ../phonebook-backend",
    "deploy": "npm run build:ui && git add . && git push",
    "lint": "eslint ."
  },
  "author": "MrJay",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "mongodb": "^6.8.1",
    "mongoose": "^8.6.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@stylistic/eslint-plugin-js": "^2.8.0",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "nodemon": "^3.0.2"
  }
}
