{
  "name": "phonebook-backend",
  "version": "1.0.0",
  "description": "Backend for phonebook",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "remove": "rm -rf ../phonebook-frontend/dist && rm -rf ../phonebook-backend/dist",
